package fr.ul.miage.projet.generated;
import java_cup.runtime.Symbol;

/* contr√¥le des erreurs de syntaxe : surcharge */
parser code {:
	public void syntax_error(Symbol cur_token){}
	public void unrecovered_syntax_error(Symbol cur_token) throws Exception{
		throw new Exception("Erreur");
	}
:}

/*action code*/
action code {:
	
:}

/* la grammaire */

terminal IDF, EGAL, VIRGULE, POINTVIRGULE, NOMBRE, RL;
terminal INT, VOID, TEST;
terminal PG, PD, AG, AD;

non terminal x;
non terminal prog, type, data, contenudata, declaration;
non terminal codes, fonction, parametres, corpsFonction;


start with x;

/*------debut-------*/


x ::= prog
		;

prog ::= data | prog data | codes
		;

data ::= type contenudata POINTVIRGULE RL
		;

contenudata ::= declaration | contenudata VIRGULE declaration
			;

declaration ::= IDF | IDF EGAL NOMBRE
			;
		
codes ::= fonction | codes fonction
		;
	
fonction ::= IDF PG parametres PD AG corpsFonction AD
			;
		
parametres ::= TEST
			;

corpsFonction ::= TEST
				;
			
type ::= INT
		| VOID
		;

/*------fin--------*/

